FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0/fins";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 296.9;

boundaryField
{
    base_fins
    {
        type            zeroGradient;
    }
    fins_to_fluid
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        Tnbr            T;
        kappaMethod     solidThermo;
        value           uniform 296.9;
    }
    fins_to_heater
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        Tnbr            T;
        kappaMethod     solidThermo;
        value           uniform 296.9;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 296.9;
    }
}


